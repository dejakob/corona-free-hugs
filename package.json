{
  "name": "corona-free-hugs",
  "version": "1.0.72",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:client-js && npm run build:server-js && npm run build:generate-static-pages && npm run build:copy:client-js && npm run build:copy:assets",
    "build:client-js": "webpack --config ./client-js/webpack.config.js",
    "build:server-js": "babel site/src --out-dir site/lib",
    "build:generate-static-pages": "node cli/build.js",
    "build:copy:client-js": "cp -R client-js/lib/* dist",
    "build:copy:assets": "cp -R static/* dist",
    "lint": "eslint './**/*.js'",
    "lint:fix": "eslint --fix './**/*.js'",
    "watch": "npm-watch"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "npm-watch": "0.6.0",
    "prettier": "^2.0.4",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  },
  "dependencies": {
    "@babel/polyfill": "7.8.7",
    "@google-cloud/storage": "4.7.0",
    "alegrify-ui": "1.0.14",
    "core-js": "3.6.5",
    "cuid": "2.1.8",
    "lsr": "2.0.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-alegrify-ui": "1.0.18",
    "react-dom": "16.13.1"
  },
  "watch": {
    "build:server-js": {
      "patterns": [
        "site/src"
      ],
      "quiet": false
    },
    "build:client-js": {
      "patterns": [
        "client-js/src"
      ],
      "quiet": false
    }
  }
}
